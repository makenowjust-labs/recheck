{
  "name": "eslint-plugin-redos",
  "version": "0.0.0",
  "description": "ESLint plugin for catching ReDoS vulnerability",
  "license": "MIT",
  "author": {
    "name": "TSUYUSATO Kitsune",
    "email": "make.just.on@gmail.com"
  },
  "homepage": "https://makenowjust-labs.github.io/recheck/",
  "repository": {
    "type": "git",
    "url": "https://github.com/makenowjust-labs/recheck.git"
  },
  "main": "lib/main.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node -r esbuild-register ./scripts/build.ts",
    "clean": "rimraf lib",
    "format": "prettier -w *.{json,md} {scripts,src}/**/*.ts",
    "lint": "prettier --check *.{json,md} {scripts,src}/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p ."
  },
  "dependencies": {
    "recheck": "0.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "@types/eslint": "8.21.3",
    "@types/find-cache-dir": "3.2.1",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.7",
    "esbuild": "0.17.12",
    "esbuild-jest": "0.5.0",
    "esbuild-register": "3.4.2",
    "eslint": "8.36.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "rimraf": "4.4.1",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "redos",
    "regexp",
    "security"
  ]
}
