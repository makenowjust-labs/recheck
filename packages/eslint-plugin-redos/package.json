{
  "name": "eslint-plugin-redos",
  "description": "ESLint plugin for catching ReDoS vulnerability",
  "license": "MIT",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "homepage": "https://makenowjust-labo.github.io/recheck/",
  "repository": "https://github.com/MakeNowJust-Labo/recheck.git",
  "version": "0.0.0-dev",
  "main": "lib/main.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node -r esbuild-register ./scripts/build.ts",
    "clean": "rimraf lib",
    "format": "prettier -w *.{json,md} {scripts,src}/**/*.ts",
    "lint": "prettier --check *.{json,md} {scripts,src}/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p ."
  },
  "dependencies": {
    "recheck": "0.0.0-dev"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/node": "16.11.22",
    "esbuild": "0.14.16",
    "esbuild-jest": "0.5.0",
    "esbuild-register": "3.3.2",
    "eslint": "8.8.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "redos",
    "regexp",
    "security"
  ]
}
