{
  "name": "eslint-plugin-redos",
  "version": "0.0.0",
  "description": "ESLint plugin for catching ReDoS vulnerability",
  "license": "MIT",
  "author": {
    "name": "TSUYUSATO Kitsune",
    "email": "make.just.on@gmail.com"
  },
  "homepage": "https://makenowjust-labs.github.io/recheck/",
  "repository": {
    "type": "git",
    "url": "https://github.com/makenowjust-labs/recheck.git"
  },
  "main": "lib/main.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "build": "node -r esbuild-register ./scripts/build.ts",
    "clean": "rimraf lib",
    "format": "prettier -w *.{json,md} {scripts,src}/**/*.ts",
    "lint": "prettier --check *.{json,md} {scripts,src}/**/*.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p ."
  },
  "dependencies": {
    "recheck": "0.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/estree": "1.0.8",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.9",
    "esbuild": "0.27.2",
    "esbuild-jest": "0.5.0",
    "esbuild-register": "3.6.0",
    "eslint": "9.39.2",
    "jest": "30.2.0",
    "prettier": "3.8.1",
    "rimraf": "6.1.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "redos",
    "regexp",
    "security"
  ]
}
