{
  "name": "recheck",
  "description": "A vulnerable RegExp (ReDoS) checker for JavaScript (ECMA-262) RegExp.",
  "license": "MIT",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "homepage": "https://makenowjust-labo.github.io/recheck/",
  "repository": "https://github.com/MakeNowJust-Labo/recheck.git",
  "version": "0.0.0-dev",
  "main": "lib/main.js",
  "browser": "lib/browser.js",
  "types": "index.d.ts",
  "keywords": [
    "redos",
    "regexp",
    "security",
    "vulnerability"
  ],
  "scripts": {
    "build": "node -r esbuild-register ./scripts/build.ts",
    "clean": "rimraf lib",
    "format": "prettier -w '*.{js,json,md}' 'src/**/*.{js,ts}' index.d.ts",
    "lint": "prettier --list-different '*.{js,json,md}' 'src/**/*.{js,ts}' index.d.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p ."
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "optionalDependencies": {
    "recheck-jar": "0.0.0-dev",
    "recheck-linux-x64": "0.0.0-dev",
    "recheck-macos-x64": "0.0.0-dev",
    "recheck-windows-x64": "0.0.0-dev"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "16.11.21",
    "esbuild": "0.14.11",
    "esbuild-jest": "0.5.0",
    "esbuild-register": "3.3.2",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.4"
  }
}
