{
  "name": "recheck",
  "description": "A vulnerable RegExp (ReDoS) checker for JavaScript (ECMA-262) RegExp.",
  "license": "MIT",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "homepage": "https://makenowjust-labo.github.io/recheck/",
  "repository": "https://github.com/MakeNowJust-Labo/recheck.git",
  "version": "4.1.1",
  "main": "lib/main.js",
  "browser": "lib/browser.js",
  "types": "index.d.ts",
  "keywords": [
    "redos",
    "regexp",
    "security",
    "vulnerability"
  ],
  "scripts": {
    "build": "esbuild --bundle --platform=node --format=cjs src/main.ts --outfile=lib/main.js && esbuild --bundle --platform=browser --format=esm src/browser.ts --outfile=lib/browser.js",
    "clean": "rimraf lib",
    "format": "prettier -w '*.{js,json,md}' 'src/**/*.ts' index.d.ts",
    "lint": "prettier --list-different '*.{js,json,md}' 'src/**/*.ts' index.d.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit -p ."
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "optionalDependencies": {
    "recheck-jar": "4.1.1",
    "recheck-linux-x64": "4.1.1",
    "recheck-macos-x64": "4.1.1",
    "recheck-windows-x64": "4.1.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.16.7",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "esbuild": "0.14.10",
    "esbuild-jest": "0.5.0",
    "jest": "27.4.6",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.4"
  }
}
